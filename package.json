{
  "name": "klubitus",
  "version": "9.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "api:build": "docker-compose build api",
    "api:precommit": "cd api && yarn precommit",
    "api:shell": "docker-compose run api sh",
    "api:up": "docker-compose up api",
    "api:yarn": "docker-compose exec api yarn",
    "client:build": "docker-compose build --no-cache client",
    "client:precommit": "cd client && yarn precommit",
    "client:shell": "docker-compose run client sh",
    "client:up": "docker-compose up client",
    "client:yarn": "docker-compose exec client yarn",
    "db:psql": "docker-compose exec db psql -U klubitus klubitus",
    "db:up": "docker-compose up db"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn api:precommit && yarn client:precommit && git -p diff --cached"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^1.3.1",
    "prettier": "^1.16.4"
  }
}
