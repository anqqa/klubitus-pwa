{
  "defaultSeverity": "warning",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "linterOptions": {
    "exclude": ["node_modules/**", ",.nuxt/**"]
  },

  "jsRules": {},
  "rules": {
    "interface-name": false,
    "member-access": false,
    "no-console": true,
    "object-literal-sort-keys": false,
    "prettier": true
  }
}
