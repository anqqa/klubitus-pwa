{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "postgres": {
    "client": "pg",
    "connection": {
      "host": "localhost",
      "user": "klubitus",
      "password": "klubitus",
      "database": "klubitus",
      "charset": "utf8"
    }
  },
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "email",
      "username"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://klubitus.org",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "email": {
      "name": "email",
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    },
    "username": {
      "name": "\\username",
      "entity": "user",
      "usernameField": "\\username",
      "passwordField": "password"
    },
    "facebook": {
      "clientID": "your facebook client id",
      "clientSecret": "your facebook client secret",
      "successRedirect": "/",
      "scope": [
        "public_profile",
        "email"
      ],
      "profileFields": [
        "id",
        "displayName",
        "first_name",
        "last_name",
        "email",
        "gender",
        "profileUrl",
        "birthday",
        "picture",
        "permissions"
      ]
    },
    "cookie": {
      "enabled": true,
      "name": "feathers-jwt",
      "httpOnly": false,
      "secure": false
    }
  }
}
