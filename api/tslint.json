{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "arrow-parens": false,
    "interface-name": [false],
    "max-line-length": [true, 100],
    "member-access": [false],
    "ordered-imports": [true],
    "quotemark": [true, "single"]
  },
  "rulesDirectory": []
}
